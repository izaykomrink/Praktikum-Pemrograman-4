<?xml version="1.0" encoding="UTF-8"?><web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">      <context-param>        <param-name>contextConfigLocation</param-name>        <param-value>/WEB-INF/konfig-spring-security.xml</param-value>        <param-value>classpath:config-spring.xml</param-value>    </context-param>		<listener>		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>	</listener>        <servlet>        <servlet-name>output</servlet-name>        <servlet-class>org.spring.framework.OutputServlet</servlet-class>    </servlet>        <servlet-mapping>        <servlet-name>output</servlet-name>        <url-pattern>/output.php</url-pattern>    </servlet-mapping>			<servlet>        <servlet-name>outputDao</servlet-name>        <servlet-class>org.spring.framework.OutputServletDao</servlet-class>    </servlet>        <servlet-mapping>        <servlet-name>outputDao</servlet-name>        <url-pattern>/outputDao.php</url-pattern>    </servlet-mapping></web-app>